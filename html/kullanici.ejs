<%- include('ek/header', {bot, user, path}) %>
<script type="text/javascript">
var title = "WordBotlist - Kullanıcı Paneli";
document.title = title;
document.getElementById("kullanici").classList.add('active');
</script>
<div class="container">
  <div align="center" class="jumbotron">
    <%
    var txt = `${user.username}#${user.discriminator} adlı kullanıcının profilini görmek için tıkla`
    if (user) {
      if (user.id === kisi.id) {
        var txt = `Profilini görmek için tıkla`
      }
    }
    %>
       <img src="https://cdn.discordapp.com/avatars/<%=user.id%>/<%=user.avatar%>.png?size=512" class="rounded-circle panell">
       <h5 style="color:#333"><%=user.username%>#<%=user.discriminator%></h5>
       <a href="/kullanici/<%=user.id%>/profil" class="btn btn-default" style="background:linear-gradient(to right,#e62f2f 0,#ff5d3d 100%); color:#fff"><%=txt%></a>
       <% if (user) { %>
       <% if (user.id === kisi.id) { %>
       <a href="/kullanici/<%=user.id%>/panel" class="btn btn-default" style="background:linear-gradient(to right,#e62f2f 0,#ff5d3d 100%); color:#fff">Panele gitmek için tıkla</a>
       <% } %>
       <% } %>
  </div>
</div>
</div></div>
<% include ./ek/footer %>